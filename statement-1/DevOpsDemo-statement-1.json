            "CidrBlock":{
               "Fn::FindInMap":[
                  {
                     "Ref":"Account"
                  },
                  "SubnetConfig",
                  "VPC"
               ]
            },
            "InstanceTenancy":"default",
            "Tags":[
               {
                  "Key":"Name",
                  "Value":{
                     "Fn::Join":[
                        ":",
                        [
                           {
                              "Ref":"VpcName"
                           },
                           {
                              "Ref":"Account"
                           }
                        ]
                     ]
                  }
               }
            ]
         }
      },
      "InternetGateway":{
